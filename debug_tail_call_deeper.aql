// Deeper tail call test

function sum_tail(n, acc) {
    if n <= 0 {
        return acc
    }
    return sum_tail(n - 1, acc + n)
}

print("Testing deeper tail call:")
let result1 = sum_tail(10, 0)
print("sum_tail(10, 0) = " + string(result1))

let result2 = sum_tail(50, 0)
print("sum_tail(50, 0) = " + string(result2))
