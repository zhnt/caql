-- AQL示例程序：Hello World
-- 展示AQL的基础语法和容器使用

print("🎉 Welcome to AQL - AI Query Language!")
print("Version: 1.0.0")

-- 基础变量声明
let name = "AQL"
let version = 1.0
let is_ai_native = true

print("Language:", name)
print("Version:", version)
print("AI Native:", is_ai_native)

-- AQL容器演示
-- 固定数组
let numbers = array<int, 5>{1, 2, 3, 4, 5}
print("Fixed Array:", numbers)

-- 动态切片  
let names = slice<string>{"Alice", "Bob", "Charlie"}
names.append("David")
print("Dynamic Slice:", names)

-- 哈希字典
let person = dict<string, any>{
    "name": "Alice",
    "age": 30,
    "city": "Shanghai"
}
print("Dictionary:", person)

-- 简单函数
function greet(name: string) -> string {
    return "Hello, " + name + "!"
}

print(greet("AQL User"))

-- 容器操作
let sum = 0
for num in numbers {
    sum = sum + num
}
print("Sum of numbers:", sum)

-- 条件判断
if sum > 10 {
    print("Sum is greater than 10")
} else {
    print("Sum is 10 or less")
}

print("🚀 AQL示例程序执行完成！") 